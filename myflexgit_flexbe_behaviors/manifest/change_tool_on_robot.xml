<?xml version="1.0" encoding="UTF-8"?>

<behavior name="Change tool on robot">

	<executable package_path="myflexgit_flexbe_behaviors.change_tool_on_robot_sm" class="ChangetoolonrobotSM" />
	<tagstring>trap, D5.2</tagstring>
	<author>Rok Pahic</author>
	<date>Fri Mar 26 2021</date>
	<description>
		Pick up heat alocator from table and put in clamp
	</description>


	<!-- Contained Behaviors -->

	<!-- Available Parameters -->
	<params>

		<param type="numeric" name="max_acl" default="0.3" label="max_acl" hint="Sets the max_acl">
			<min value="0" />
			<max value="1" />
		</param>

		<param type="text" name="namespace" default="panda_2" label="namespace" hint="Sets the namespace" />

		<param type="numeric" name="max_vel" default="0.3" label="max_vel" hint="Sets the max_vel">
			<min value="0" />
			<max value="1" />
		</param>

		<param type="text" name="air_block_service_name" default="/Panda2BlockON" label="air_block_service_name" hint="Sets the air_block_service_name" />

		<param type="text" name="tool_unlock_service_name" default="/Panda2ToolUnlock" label="tool_unlock_service_name" hint="Sets the tool_unlock_service_name" />

	</params>


</behavior>